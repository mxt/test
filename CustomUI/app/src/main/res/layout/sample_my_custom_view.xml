<?xml version="1.0" encoding="utf-8"?>
<com.carrey.customview.customview.CustomView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:carrey="http://schemas.android.com/apk/res/com.team.customui"
    android:id="@+id/MyCustomView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerInParent="true"
    android:background="#FFD700"
    carrey:imageAspectRatio="1.0"
    carrey:imageAlpha="1.0"
    carrey:imagePaddingLeft="1dp"
    carrey:imagePaddingTop="1dp"
    carrey:imagePaddingRight="1dp"
    carrey:imagePaddingBottom="1dp"
    carrey:imageScaleType="center"
    carrey:xMessage="view"
  />
